<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="wooyun.esnb"
    android:versionCode="38"
    android:versionName="2.6.2">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="wooyun.esnb.application.App"
        android:allowBackup="true"
        android:icon="@drawable/icon_t"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_t"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning"
        tools:replace="android:allowBackup"
        tools:targetApi="m">
        <activity android:name="wooyun.esnb.activity.AboutActivity"></activity>
        <activity
            android:name="wooyun.esnb.activity.SettingActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="wooyun.esnb.activity.KyActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="wooyun.esnb.activity.MainActivity"
            android:launchMode="singleTask"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="wooyun.esnb.activity.EditActivity"
            android:label="@string/Active_page0"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="wooyun.esnb.activity.WelcomeActivity"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="wooyun.esnb.activity.ShowActivity"
            android:label="@string/Active_page1"
            tools:ignore="InnerclassSeparator" />
    </application>

</manifest>